CREATE TABLE "projetos_calculos" (
	"id"	INTEGER NOT NULL,
	"id_projetos"	INTEGER NOT NULL DEFAULT 0,
	"id_estacas_diametros"	INTEGER NOT NULL DEFAULT 0,
	"id_solos"	INTEGER NOT NULL DEFAULT 0,
	"id_metodos"	INTEGER NOT NULL,
	"profcamada"	NUMERIC NOT NULL DEFAULT 0,
	"spt_medio1"	NUMERIC NOT NULL DEFAULT 0,
	"spt_medio_calculo"	TEXT,
	"fator_solo"	NUMERIC NOT NULL DEFAULT 0,
	"r1"	NUMERIC NOT NULL DEFAULT 0,
	"r1_calculo"	TEXT,
	"r2"	NUMERIC NOT NULL DEFAULT 0,
	"r2_calculo"	TEXT,
	"calc_r1"	NUMERIC NOT NULL DEFAULT 0,
	"calc_r1_txt"	TEXT,
	"calc_k"	NUMERIC NOT NULL DEFAULT 0,
	"calc_rp"	NUMERIC NOT NULL DEFAULT 0,
	"calc_rp_txt"	TEXT,
	"tot_kn"	NUMERIC NOT NULL DEFAULT 0,
	CONSTRAINT "FK3_projetos_calculos" FOREIGN KEY("id_estacas_diametros") REFERENCES "estacas_diametros"("id") ON DELETE RESTRICT,
	CONSTRAINT "FK4_projetos_calculos" FOREIGN KEY("id_metodos") REFERENCES "metodos"("id") ON DELETE RESTRICT,
	CONSTRAINT "FK1_projetos_calculos" FOREIGN KEY("id_projetos") REFERENCES "projetos"("id") ON DELETE CASCADE,
	CONSTRAINT "FK2_projetos_calculos" FOREIGN KEY("id_solos") REFERENCES "solos"("id") ON DELETE RESTRICT,
	PRIMARY KEY("id" AUTOINCREMENT)
)
